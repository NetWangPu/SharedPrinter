.swiper{
    width: 100%;
    height: 200px;
    /* 圆角 */
    border-radius: 10px;
    overflow: hidden;
}
.swiper-img{
    width: 100%;
    height: 200px;
    /* 图片自适应 */
    object-fit: cover;
}
.list-title{
    font-size: 20px;
    font-weight: bold;
    margin: 10px 0;
    /* 居中 */
    text-align: center;
}
.list-len{
    font-size: 14px;
    color: #999;
    margin: 10px 0;
    /* 居中 */
    text-align: center;
}
.list{
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    padding: 0 10px;
}

.list-item{
    width: 100%;
    height: 120px;
    margin-bottom: 10px;
    /* 圆角 */
    border-radius: 10px;
    /* 阴影 */
    box-shadow: 0 0 10px #ccc;
    /* 边框 */
    border: 1px solid #4b4848;
}
/* print-img在最左边list-item-time、list-item-lng、list-item-lat在最右边 */
.print-img{
    width: 30%;
    height: 120px;
    /* 图片自适应 */
    object-fit: cover;
    /* 左侧对齐 */
    float: left;
}
.list-item-info{
    width: 70%;
    height: 120px;
    /* 左侧对齐 */
    float: left;
}
.list-item-time{
    font-size: 13px;
    color: #999;
    margin: 10px 0;
    /* 居中 */

}
.list-item-lng{
    font-size: 16px;
    color: #191818;
    margin: 10px 0;
}

.list-item-lat{
    font-size: 12px;
    color: #0f0823;
    margin: 10px 0;
}
.scan-button {
    /* 悬浮: 右下角; */
    position: fixed;
    bottom: 0;
    right: 0;
    /* 大小: 60px * 60px; */
    width: 80px;
    height: 30px;
    /* 背景: 红色; */
    background-color: rgb(0, 255, 85);
    /* 圆角: 30px; */
    border-radius: 50px;
    /* 内边距: 10px; */
    padding: 10px;
    /* 边框: 20px solid #ccc; */
    border: 1px solid #ccc;
    /* 阴影: 0 0 5px #ccc; */
    box-shadow: 0 0 5px #ccc;
    /* 文字: 居中; */
    text-align: center;
    /* 文字: 白色; */
    color: #fff;
    /* 文字: 14px; */
    font-size: 14px;
    /* 文字: 加粗; */
    font-weight: bold;
    /* 文字: 阴影; */
    text-shadow: 0 0 5px #ccc;
    /*外边距: 10px;*/
    margin: 10px;
    /*最上层*/
    z-index: 999;
}
