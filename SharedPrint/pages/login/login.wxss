.login-btn{
    width: 100%;
    height: 40px;
    line-height: 20px;
    background-color: rgb(155, 235, 180);
    color: #313131;
    text-align: center;
    border-radius: 5px;
    margin-top: 50px;
}
.title{
    text-align: center;
    margin-top: 50px;
    margin-bottom: 80px;
}
.title-text{
    font-size: 20px;
    color: #3c3535;
    font-weight: 800;
