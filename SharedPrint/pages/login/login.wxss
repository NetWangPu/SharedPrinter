}
.inputs{
    /* 外边框 */
    border: 1px solid #ccc;
    /* 圆角 */
    border-radius: 5px;
    /* 内边距 */
    padding: 0 10px;
    /* 背景色 */
    background-color: #1fd62e;
    
}
